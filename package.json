{
    "name": "rc-map",
    "dependencies": {
        "@babel/polyfill": "7.4.4",
        "cheerio": "1.0.0-rc.3",
        "cordova-android": "8.0.0",
        "cordova-browser": "6.0.0",
        "cordova-custom-config": "5.1.0",
        "cordova-ios": "5.0.1",
        "cordova-plugin-mapbox": "1.2.3",
        "cordova-plugin-splashscreen": "5.0.3",
        "cordova-plugin-webpack": "0.4.6",
        "cordova-plugin-whitelist": "1.3.4",
        "csp-parse": "0.0.2",
        "monaca-plugin-monaca-core": "3.3.0",
        "node-sass": "4.12.0",
        "node-sass-import": "2.0.1",
        "npm-run-all": "4.1.5",
        "postcss": "7.0.17",
        "postcss-cli": "6.1.3"
    },
    "browserslist": [
        "last 2 versions",
        "not ie < 11",
        "Android >= 6",
        "iOS >= 10"
    ],
    "cordova": {
        "plugins": {
            "cordova-custom-config": {},
            "cordova-plugin-splashscreen": {},
            "cordova-plugin-whitelist": {},
            "monaca-plugin-monaca-core": {}
        },
        "platforms": [
            "browser"
        ]
    },
    "scripts": {
        "commit": "node ./node_modules/cz-customizable/standalone.js",
        "monaca:preview": "npm run dev",
        "dev": "browser-sync start -s www/ --watch --port 8080 --ui-port 8081",
        "scss": "node-sass --watch --include-path scss www/scss -o www/css --output-style compressed",
        "prefix": "postcss --use autoprefixer --no-map www/css -d www/css",
        "test": "npm-run-all -p dev scss prefix",
        "gulp": "gulp browserify",
        "uncss": "grunt default",
        "webpack": "cordova build --browserify && cordova run-- --livereload",
        "build": "cordova build --browserify"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/yoshi1125hisa/rc-map_front.git"
    },
    "author": "yoshi1125hisa",
    "license": "UNLICENSED",
    "devDependencies": {
        "cz-customizable": "6.2.0",
        "@babel/core": "7.5.5",
        "autoprefixer": "10.2.4",
        "babel-loader": "8.0.6",
        "babel-preset-es2015": "6.24.1",
        "babelify": "10.0.0",
        "browser-sync": "2.26.7",
        "browserify": "16.5.0",
        "cordova": "9.0.0",
        "css-loader": "3.2.0",
        "eslint": "6.3.0",
        "eslint-config-standard": "14.1.0",
        "eslint-plugin-import": "2.18.2",
        "eslint-plugin-node": "9.2.0",
        "eslint-plugin-promise": "4.2.1",
        "eslint-plugin-standard": "4.0.1",
        "grunt": "1.0.4",
        "gulp": "4.0.2",
        "gulp-postcss": "8.0.0",
        "html-loader": "0.5.5",
        "license-list": "0.1.3",
        "postcss-import": "12.0.1",
        "postcss-loader": "3.0.0",
        "style-loader": "1.0.0",
        "svg-inline-loader": "0.8.0",
        "vinyl-source-stream": "2.0.0",
        "webpack": "4.39.3",
        "webpack-cli": "3.3.7",
        "webpack-dev-server": "3.8.0"
    }
}
